            <input type="number" name="Party Reputation" class="party-reputation-text" id="reputation" value="{{eputation}}">

            <label for="shop-modifier-text" class="modifier-label">Shop Modifier: </label>
            <label for="shop-modifier-text" id="modifier">{{shopModifier}}</label>

        </fieldset>
        <table class="player-characters">
            <!-- If JS function, then enters sum level of players and number of players, gets level-->
