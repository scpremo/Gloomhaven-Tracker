                {{/each}}
            </tr>
            <tr>
                {{#each playerData3To4}}
                {{> playerinfo  }}
                {{/each}}
            </tr>
            <tr>
                <td>
                    <div> </div>
                </td>
                <td>
                    <div>Average Party Level: </div>
                </td>
                <td>
                    <div class="avg-level" id="avg-level">{{averageLevel}}</div>
                </td>
                <td>
                    <div> </div>
                </td>
                <td>
                    <div>
                        <select class="scenario-level" name="scenario-level" id="dificulty">
                            <option value="-1">Easy</option>
                            <option selected value="0">Normal</option>
                            <option value="1">Hard</option>
                            <option value="2">Very Hard</option>
                        </select>
